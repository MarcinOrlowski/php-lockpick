#
##################################################################################

parameters:
  level: max
  paths:
    - src/
    - tests/
  ignoreErrors:
        -
            message: '#Parameter \#1 \$objectOrClass of class ReflectionClass constructor expects class-string<.*>|T of object, object\|string given\.#'
            path: '%currentWorkingDirectory%/src/Lockpick.php'
