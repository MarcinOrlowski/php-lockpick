{
    "name": "marcin-orlowski/lockpick",
    "description": "Helps accessing protected/private members/consts of foreign objects.",
    "homepage": "https://github.com/MarcinOrlowski/php-lockpick",
    "version": "1.4.0",
    "keywords": [
        "helper",
        "php8",
        "private",
        "protected",
        "constants"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Orlowski",
            "homepage": "https://MarcinOrlowski.com/"
        }
    ],
    "replace": {
        "marcin-orlowski/php-lockpick": "*"
    },
    "conflicts": {
	"marcin-orlowski/php-lockpick": "*"
    },
    "support": {
        "source": "https://github.com/MarcinOrlowski/php-lockpick/",
        "issues": "https://github.com/MarcinOrlowski/php-lockpick/issues"
    },
    "autoload": {
        "psr-4": {
            "MarcinOrlowski\\Lockpick\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcinOrlowski\\LockpickTest\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "marcin-orlowski/type-asserts": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-code-coverage": "^9.0",
        "phpstan/phpstan": "^2.0"
    },
    "scripts": {
	  "test": "vendor/bin/phpunit -c phpinit.xml -c phpunit.xml",

      "lint": [
        "@composer codelint",
        "@composer mdlint"
      ],
      "codelint": "vendor/bin/phpstan analyse -c phpstan.neon",
      "mdlint": "markdownlint --config .markdownlint.yaml --ignore vendor --ignore CLAUDE.md ignore .claude/**/*.md **/*.md .github/**/*.md docs README.md"
    }
}
